{
    "Function Design Recipe": {
        "prefix": "recipe",
        "body": [
            "def ${1:function_name}(${2:parameters}):",
            "    '''${3:type_signature}",
            "    ",
            "    ${4:description}",
            "    ",
            "    >>> ${5:example}",
            "    '''",
            "    ${6:body}"
        ]
    },
    "For loop reduce": {
        "prefix": "forreduce",
        "body": [
            "${1:result} = ${2:initial_value}",
            "for ${3:element} in ${4:collection}:",
            "    ${1:result} = ${5:reduction}(result, ${3:element})"
        ]
    },
    "For loop filter": {
        "prefix": "forfilter",
        "body": [
            "${1:result} = []",
            "for ${2:element} in ${3:collection}:",
            "    if ${4:condition}(${2:element}):",
            "        ${1:result}.append(${2:element})"
        ]
    },
    "For loop map": {
        "prefix": "formap",
        "body": [
            "${1:result} = []",
            "for ${2:element} in ${3:collection}:",
            "    ${1:result}.append(${4:mapper}(${2:element}))"
        ]
    }
}