{
    "name": "base-camp-coding-academy",
    "displayName": "Base Camp Coding Academy",
    "description": "Helpful tools for Base Camp Coding Academy students",
    "version": "1.1.0",
    "publisher": "natec425",
    "engines": {
        "vscode": "^1.12.0"
    },
    "categories": [
        "Snippets",
        "Extension Packs"
    ],
    "contributes": {
        "snippets": [
            {
                "language": "python",
                "path": "./snippets/snippets.json"
            }
        ],
        "configurationDefaults": {
            "python.pythonPath": "python3",
            "python.linting.pylintEnabled": false
        }
    },
    "extensionDependencies": [
        "natec425.doctest-this-file",
        "donjayamanne.python",
        "magicstack.magicpython",
        "christian-kohler.path-intellisense"
    ]
}